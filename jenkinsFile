                        bat "mvn -f C:\\Users\\E-LINE\\Desktop\\GapStars\\reWtiteConxillium test"
                    }
                }
                stage('Compile'){
                    steps{
                        echo "Building the Project"
                        bat "mvn -f C:\\Users\\E-LINE\\Desktop\\GapStars\\reWtiteConxillium compile"
                    }
                }
}
}